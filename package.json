{
  "name": "planmill-api",
  "version": "1.5.0",
  "description": "PlanMill API documentation",
  "main": "index.js",
  "scripts": {
    "lint": "jlint --glob './*.sample' && jlint --glob './*.schema'",
    "build": "npm i -g https://github.com/planmill/raml2html.git#develop --force & raml2html api_docs/planmill1_5.raml > api_docs/index.html & node -e require('./lambda/schema/server.js').init()",
    "patch": "npm run build & npm version patch & npm publish"
